# These are the Xn inputs done with bit access X0-X7
file "../../db/bi_bit.template" { pattern
{P,           R,       PORT,          OFFSET,   ZNAM,   ONAM,  ZSV,       OSV,    SCAN}
{KOYO:K1:,    X0B,     K1_Xn_Bit,     0,        Low,    High,  NO_ALARM,  MAJOR,  "I/O Intr"}
{KOYO:K1:,    X1B,     K1_Xn_Bit,     1,        Low,    High,  NO_ALARM,  MAJOR,  "I/O Intr"}
{KOYO:K1:,    X2B,     K1_Xn_Bit,     2,        Low,    High,  NO_ALARM,  MAJOR,  "I/O Intr"}
{KOYO:K1:,    X3B,     K1_Xn_Bit,     3,        Low,    High,  NO_ALARM,  MAJOR,  "I/O Intr"}
{KOYO:K1:,    X4B,     K1_Xn_Bit,     4,        Low,    High,  NO_ALARM,  MAJOR,  "I/O Intr"}
{KOYO:K1:,    X5B,     K1_Xn_Bit,     5,        Low,    High,  NO_ALARM,  MAJOR,  "I/O Intr"}
{KOYO:K1:,    X6B,     K1_Xn_Bit,     6,        Low,    High,  NO_ALARM,  MAJOR,  "I/O Intr"}
{KOYO:K1:,    X7B,     K1_Xn_Bit,     7,        Low,    High,  NO_ALARM,  MAJOR,  "I/O Intr"}
}
file "../../db/longin.template" { pattern
{P,           R,       PORT,           OFFSET,   SCAN}
{KOYO:K1:,    X0L,     K1_Xn_Bit,      0,        "I/O Intr"}
}
file "../../db/intarray_in.template" { pattern
{P,           R,        PORT,           NELM,   SCAN}
{KOYO:K1:,    XnBArray, K1_Xn_Bit,      32,     ".1 second"}
}
file "../../db/asynRecord.template" { pattern
{P,           R,       PORT,          ADDR,     TMOD,  IFACE}
{KOYO:K1:,    XnBAsyn, K1_Xn_Bit,     0,        Read,  asynUInt32Digital}
}
file "../../db/poll_delay.template" { pattern
{P,           R,            PORT}
{KOYO:K1:,    XnBPollDelay, K1_Xn_Bit}
}


# These are the Xn inputs done with word access X0-X7
file "../../db/bi_word.template" { pattern
{P,           R,       PORT,           OFFSET,    MASK,     ZNAM,   ONAM,  ZSV,       OSV,    SCAN}
{KOYO:K1:,    X0W,     K1_Xn_Word,     0,        0x0001,    Low,    High,  NO_ALARM,  MAJOR,  "I/O Intr"}
{KOYO:K1:,    X1W,     K1_Xn_Word,     0,        0x0002,    Low,    High,  NO_ALARM,  MAJOR,  "I/O Intr"}
{KOYO:K1:,    X2W,     K1_Xn_Word,     0,        0x0004,    Low,    High,  NO_ALARM,  MAJOR,  "I/O Intr"}
{KOYO:K1:,    X3W,     K1_Xn_Word,     0,        0x0008,    Low,    High,  NO_ALARM,  MAJOR,  "I/O Intr"}
{KOYO:K1:,    X4W,     K1_Xn_Word,     0,        0x0010,    Low,    High,  NO_ALARM,  MAJOR,  "I/O Intr"}
{KOYO:K1:,    X5W,     K1_Xn_Word,     0,        0x0020,    Low,    High,  NO_ALARM,  MAJOR,  "I/O Intr"}
{KOYO:K1:,    X6W,     K1_Xn_Word,     0,        0x0040,    Low,    High,  NO_ALARM,  MAJOR,  "I/O Intr"}
{KOYO:K1:,    X7W,     K1_Xn_Word,     0,        0x0080,    Low,    High,  NO_ALARM,  MAJOR,  "I/O Intr"}
}
file "../../db/longin.template" { pattern
{P,           R,       PORT,           OFFSET,   SCAN}
{KOYO:K1:,    XnL,     K1_Xn_Word,     0,        "I/O Intr"}
}
file "../../db/intarray_in.template" { pattern
{P,           R,        PORT,           NELM,   SCAN}
{KOYO:K1:,    XnWArray, K1_Xn_Word,     8,     ".1 second"}
}
file "../../db/asynRecord.template" { pattern
{P,           R,       PORT,          ADDR,     TMOD,  IFACE}
{KOYO:K1:,    XnWAsyn, K1_Xn_Word,    0,        Read,  asynInt32}
}
file "../../db/histogram.template" { pattern
{P,           R,       PORT,       SCAN}
{KOYO:K1:,    XnWHist, K1_Xn_Word, "10 second"}
}
file "../../db/poll_delay.template" { pattern
{P,           R,            PORT}
{KOYO:K1:,    XnWPollDelay, K1_Xn_Word}
}

