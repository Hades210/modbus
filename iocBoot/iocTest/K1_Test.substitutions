file "../../db/bo_bit.template"
{
pattern
# These are the Yn outputs done with bit access.  Y0-Y7 
{P,           R,          PORT,              OFFSET,   ZNAM,   ONAM}
{KOYU:K1:,    Y0OutB,     K1_Yn_Out_Bit,     0,        Low,    High}
{KOYU:K1:,    Y1OutB,     K1_Yn_Out_Bit,     1,        Low,    High}
{KOYU:K1:,    Y2OutB,     K1_Yn_Out_Bit,     2,        Low,    High}
{KOYU:K1:,    Y3OutB,     K1_Yn_Out_Bit,     3,        Low,    High}
{KOYU:K1:,    Y4OutB,     K1_Yn_Out_Bit,     4,        Low,    High}
{KOYU:K1:,    Y5OutB,     K1_Yn_Out_Bit,     5,        Low,    High}
{KOYU:K1:,    Y6OutB,     K1_Yn_Out_Bit,     6,        Low,    High}
{KOYU:K1:,    Y7OutB,     K1_Yn_Out_Bit,     7,        Low,    High}
}

