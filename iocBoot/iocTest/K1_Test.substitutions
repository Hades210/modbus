file "../../db/bi_bit.template"
{
pattern
# These are the Xn inputs done with bit access X0-X7
{P,           R,       PORT,          OFFSET,   ZNAM,   ONAM,  ZSV,       OSV,    SCAN}
{KOYU:K1:,    X0B,     K1_Xn_Bit,     0,        Low,    High,  NO_ALARM,  MAJOR,  ".1 second"}
{KOYU:K1:,    X1B,     K1_Xn_Bit,     1,        Low,    High,  NO_ALARM,  MAJOR,  ".1 second"}
{KOYU:K1:,    X2B,     K1_Xn_Bit,     2,        Low,    High,  NO_ALARM,  MAJOR,  ".1 second"}
{KOYU:K1:,    X3B,     K1_Xn_Bit,     3,        Low,    High,  NO_ALARM,  MAJOR,  ".1 second"}
{KOYU:K1:,    X4B,     K1_Xn_Bit,     4,        Low,    High,  NO_ALARM,  MAJOR,  ".1 second"}
{KOYU:K1:,    X5B,     K1_Xn_Bit,     5,        Low,    High,  NO_ALARM,  MAJOR,  ".1 second"}
{KOYU:K1:,    X6B,     K1_Xn_Bit,     6,        Low,    High,  NO_ALARM,  MAJOR,  ".1 second"}
{KOYU:K1:,    X7B,     K1_Xn_Bit,     7,        Low,    High,  NO_ALARM,  MAJOR,  ".1 second"}
}

file "../../db/bi_word.template"
{
pattern
# These are the Xn inputs done with word access X0-X7
{P,           R,       PORT,           OFFSET,    MASK,     ZNAM,   ONAM,  ZSV,       OSV,    SCAN}
{KOYU:K1:,    X0W,     K1_Xn_Word,     0,        0x0001,    Low,    High,  NO_ALARM,  MAJOR,  ".1 second"}
{KOYU:K1:,    X1W,     K1_Xn_Word,     0,        0x0002,    Low,    High,  NO_ALARM,  MAJOR,  ".1 second"}
{KOYU:K1:,    X2W,     K1_Xn_Word,     0,        0x0004,    Low,    High,  NO_ALARM,  MAJOR,  ".1 second"}
{KOYU:K1:,    X3W,     K1_Xn_Word,     0,        0x0008,    Low,    High,  NO_ALARM,  MAJOR,  ".1 second"}
{KOYU:K1:,    X4W,     K1_Xn_Word,     0,        0x0010,    Low,    High,  NO_ALARM,  MAJOR,  ".1 second"}
{KOYU:K1:,    X5W,     K1_Xn_Word,     0,        0x0020,    Low,    High,  NO_ALARM,  MAJOR,  ".1 second"}
{KOYU:K1:,    X6W,     K1_Xn_Word,     0,        0x0040,    Low,    High,  NO_ALARM,  MAJOR,  ".1 second"}
{KOYU:K1:,    X7W,     K1_Xn_Word,     0,        0x0080,    Low,    High,  NO_ALARM,  MAJOR,  ".1 second"}
}

file "../../db/bi_bit.template"
{
pattern
# These are the Yn inputs done with bit access Y0-Y7
{P,           R,         PORT,             OFFSET,   ZNAM,   ONAM,  ZSV,       OSV,    SCAN}
{KOYU:K1:,    Y0InB,     K1_Yn_In_Bit,     0,        Low,    High,  NO_ALARM,  MAJOR,  ".1 second"}
{KOYU:K1:,    Y1InB,     K1_Yn_In_Bit,     1,        Low,    High,  NO_ALARM,  MAJOR,  ".1 second"}
{KOYU:K1:,    Y2InB,     K1_Yn_In_Bit,     2,        Low,    High,  NO_ALARM,  MAJOR,  ".1 second"}
{KOYU:K1:,    Y3InB,     K1_Yn_In_Bit,     3,        Low,    High,  NO_ALARM,  MAJOR,  ".1 second"}
{KOYU:K1:,    Y4InB,     K1_Yn_In_Bit,     4,        Low,    High,  NO_ALARM,  MAJOR,  ".1 second"}
